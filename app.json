{
  "expo": {
    "name": "Nexo",
    "slug": "Nexo",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "nexo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "experiments": {
      "reactCompiler": true
    }
  }
}
